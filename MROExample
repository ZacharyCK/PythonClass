 #MRO - Method Resolution Order
class A:
   num = 10

class B(A):
   pass

class C(A):
   num = 1

class D(B, C):
   pass

print(D.mro()) # Shows the order that each parent class is referenced

  # Structure for these classes with A being the parent
  #     A
  #   /   \
  #  /     \
  # B       C
  #  \     /
  #   \   /
  #     D
  


class X:pass
class Y:pass
class Z:pass
class A(X,Y):pass
class B(Y,Z):pass
class M(B,A,Z):pass

print(M.__mro__) # Order: M, B, A, X, Y, Z, object
# The order is sorted by Depth First Search
